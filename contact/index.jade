		.panel.panel-info
			.panel-heading
				.panel-title Contact Us
			.panel-body
				.form-horizontal(role='form')
					.form-group
						label.col-sm-2.control-label Email
						.col-sm-10
							span.form-control#bos-email bookofsatoshi@gmail.com


	| &nbsp;

